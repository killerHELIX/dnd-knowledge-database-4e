SELECT name FROM feats WHERE nopres=true or level <= 10 or str <= 13 or con <= 15 or dex <= 18 or int <= 16 or wis <= 15 or cha <= 8 or class='Ranger' or race = 'Dwarf' or acrobatics=false or arcana=false or athletics=false or bluff=false or dungeoneering=false or endurance=false or heal=false or intimidate=true or nature=true or perception=true or religion=false or stealth=true or feat similar to '()+' or god='Raven Queen';

SELECT name FROM feats WHERE nopres=true and level <= 10 and (str <= 13 and con <= 15 and dex <= 18 and int <= 16 and wis <= 15 and cha <= 8) or class='Ranger' or race = 'Dwarf' or (acrobatics=false and arcana=false and athletics=false and bluff=false and dungeoneering=false and endurance=false and heal=false and intimidate=true and nature=true and perception=true and religion=false and stealth=true) or feat similar to '()+' and god='Raven Queen' or armor_proficiency='Hide';