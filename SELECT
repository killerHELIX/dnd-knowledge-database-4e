SELECT name FROM feats WHERE nopres=true or level <= 10 or str <= 13 or con <= 15 or dex <= 18 or int <= 16 or wis <= 15 or cha <= 8 or class='Ranger' or race = 'Dwarf' or acrobatics=false or arcana=false or athletics=false or bluff=false or dungeoneering=false or endurance=false or heal=false or intimidate=true or nature=true or perception=true or religion=false or stealth=true or feat similar to '()+' or god='Raven Queen';